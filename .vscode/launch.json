    "version": "0.2.0",
    "configurations": [


        {
            "name": "Launch harness",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceFolder}/cmd/harness",
            "program": "${workspaceFolder}/cmd/harness/main.go"
        },
        {
            "name": "Launch api server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceFolder}/cmd/api",
            "program": "${workspaceFolder}/cmd/api/main.go"
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/utils/init/aws_initialize.go",
            "env": {},
            "args": []
        },
        {
            "name": "Connect to delve server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 2345,
            "host": "127.0.0.1",
            "showLog": true,
            "substitutePath": [
                {
                    "from":"${workspaceFolder}/cmd",
                    "to":"/src"
                }
            ]
        }
    ]
}